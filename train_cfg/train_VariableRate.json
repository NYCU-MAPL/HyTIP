{   "phase": {
    "trainRNN_GainUnit_weight": 8,
    "trainRNN_ME_weight": 30
    },

"trainRNN_GainUnit_weight": {
    "batch_size": 8, 
    "lr": 1e-5, 
    "RNN": 1, 
    "strategy": {"stage": "fullgop", "random": 1}, 
    "frozen_modules": ["if_model", "MENet", "CondMotion", "Residual"],
    "re-activate_modules": [
        ["CondMotion", "mv_q_encoder"], ["CondMotion", "mv_q_decoder"], 
        ["Residual", "q_encoder"], ["Residual", "q_decoder"], ["Residual", "q_feature"], ["Residual", "q_recon"]
    ],
    "loss_on": {"R": "rate", "D": "frame_weight*distortion"},
    "lr_scheduler": {"milestones": [], "gamma": 0.1},
    "max_num_Pframe": 4,
    "train_len": 0.5
    },

"trainRNN_ME_weight": {
    "batch_size": 8, 
    "lr": 1e-5, 
    "RNN": 1, 
    "strategy": {"stage": "fullgop"}, 
    "frozen_modules": ["if_model"],
    "re-activate_modules": [],
    "loss_on": {"R": "rate", "D": "frame_weight*distortion"},
    "lr_scheduler": {"milestones": [], "gamma": 0.1},
    "max_num_Pframe": 4,
    "train_len": 0.5
    }
}